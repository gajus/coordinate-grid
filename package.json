{
  "author": {
    "email": "gajus@gajus.com",
    "name": "Gajus Kuizinas",
    "url": "http://gajus.com"
  },
  "ava": {
    "require": [
      "@babel/register"
    ]
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "cli-boxes": "^2.2.0",
    "i": "^0.3.6",
    "lodash": "^4.17.11",
    "npm": "^6.9.0",
    "strip-ansi": "^5.2.0"
  },
  "description": "Generates a coordinate grid (CLI square grid).",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/node": "^7.2.2",
    "@babel/plugin-transform-flow-strip-types": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/register": "^7.4.4",
    "ava": "^1.4.1",
    "babel-plugin-istanbul": "^5.1.4",
    "babel-plugin-transform-export-default-name": "^2.0.4",
    "coveralls": "^3.0.3",
    "eslint": "^5.16.0",
    "eslint-config-canonical": "^17.1.0",
    "flow-bin": "^0.98.1",
    "flow-copy-source": "^2.0.6",
    "husky": "^2.3.0",
    "nyc": "^14.1.1",
    "semantic-release": "^15.13.12"
  },
  "engines": {
    "node": ">=6.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run test && npm run build"
    }
  },
  "keywords": [
    "cli",
    "ascii",
    "square",
    "coordinate",
    "grid",
    "terminal"
  ],
  "main": "./dist/index.js",
  "name": "coordinate-grid",
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "instrument": false,
    "reporter": [
      "text-lcov"
    ],
    "require": [
      "@babel/register"
    ],
    "sourceMap": false
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:gajus/coordinate-grid.git"
  },
  "scripts": {
    "build": "rm -fr ./dist && NODE_ENV=production babel ./src --out-dir ./dist --copy-files --source-maps && flow-copy-source src dist",
    "dev": "NODE_ENV=production babel ./src --out-dir ./dist --copy-files --source-maps --watch",
    "lint": "eslint ./src ./test && flow",
    "test": "NODE_ENV=test ava --serial --verbose"
  },
  "version": "1.1.0"
}
